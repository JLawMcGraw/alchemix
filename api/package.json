{
  "name": "alchemix-api",
  "version": "1.0.0",
  "description": "AlcheMix Express API backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "clear-memmachine": "tsx scripts/clear-memmachine.ts",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:backup:compress": "tsx scripts/backup-database.ts --compress --keep=30",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/utils/ src/middleware/",
    "test:db": "vitest run src/database/",
    "test:routes": "vitest run src/routes/",
    "test:all": "npm run test:unit && npm run test:db && npm run test:routes",
    "test:utils": "vitest run src/utils/",
    "test:middleware": "vitest run src/middleware/",
    "test:auth": "vitest run src/routes/auth.test.ts"
  },
  "keywords": [
    "express",
    "cocktail",
    "api"
  ],
  "author": "Jacob Lawrence",
  "license": "UNLICENSED",
  "dependencies": {
    "@alchemix/types": "file:../packages/types",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "pg": "^8.19.0",
    "resend": "^6.6.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.18.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^1.6.1",
    "supertest": "^6.3.4",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}
