{
  "name": "alchemix-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001 --turbo",
    "dev:api": "cd api && npm run dev",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev\" --names \"API,WEB\" --prefix-colors \"cyan,magenta\"",
    "dev:clean": "kill-ports.bat && npm run dev:all",
    "build": "next build && cd api && npm run build",
    "build:api": "cd api && npm run build",
    "start": "next start -p 3001",
    "start:api": "cd api && npm start",
    "lint": "next lint",
    "type-check": "tsc --noEmit && cd api && npm run type-check",
    "install:all": "npm install && cd api && npm install"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "lucide-react": "^0.553.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "typescript": "^5.3.3"
  }
}
